import PySimpleGUI as sg

# ウィンドウのレイアウトを定義
layout = [[sg.Text("Hello World")],
          [sg.Button("OK")]]

# ウィンドウを作成
window = sg.Window("My First GUI", layout)

# イベントループ
while True:
    event, values = window.read()
    if event == sg.WINDOW_CLOSED or event == "OK":
        break

window.close()

