import random

# 外部から得たアルファベットの羅列
alphabet_string = "ABCDEFGHIJKLMNOPQRSTUVWXYZ"
external_alphabet = list(alphabet_string)

# ポリビウス暗号のテーブル
polybius_table = {
    '11': 'A', '12': 'B', '13': 'C', '14': 'D', '15': 'E',
    '21': 'F', '22': 'G', '23': 'H', '24': 'I/J', '25': 'K',
    '31': 'L', '32': 'M', '33': 'N', '34': 'O', '35': 'P',
    '41': 'Q', '42': 'R', '43': 'S', '44': 'T', '45': 'U',
    '51': 'V', '52': 'W', '53': 'X', '54': 'Y', '55': 'Z'
}

# ポリビウス暗号のテーブルの数字とアルファベットの組み合わせを変更する
new_polybius_table = {}
random.shuffle(external_alphabet)  # アルファベットリストをランダムにシャッフル

# アルファベットリストとポリビウス暗号の数字を1対1で対応付け
for i, digit in enumerate(polybius_table):
    new_polybius_table[digit] = external_alphabet[i]

# 新しいポリビウス暗号のテーブルを表示
print(new_polybius_table)


